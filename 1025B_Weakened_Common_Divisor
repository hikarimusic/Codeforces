//Codeforces 1025B - Weakened Common Divisor

#include <iostream>
#include <vector>

using namespace std;
typedef long long ll ;

void add_fac(ll n, vector<ll>& fac) {
    if (n%2==0) {
        fac.push_back(2) ;
        while (n%2==0) n/=2 ;
    }
    for (ll i=3;i*i<=n;i+=2) {
        if (n%i!=0) continue ;
        fac.push_back(i) ;
        while (n%i==0) {
            n/=i ;
        }
    }
    if (n>1) fac.push_back(n) ;
}

int main()
{
    ll n ;
    cin>>n ;
    ll a, b ;
    cin>>a>>b ;
    vector<ll> fac ;
    add_fac(a, fac) ;
    add_fac(b, fac) ;
    for (int i=0;i<n-1;++i) {
        cin>>a>>b ;
        for (int j=0;j<fac.size();) {
            if (a%fac[j] == 0 || b%fac[j] == 0) {
                j+=1 ;
                continue ;
            }
            fac.erase(fac.begin()+j) ;
        }
    }
    if (fac.size()) cout<<fac[0] ;
    else cout<<-1 ;
    return 0;
}

