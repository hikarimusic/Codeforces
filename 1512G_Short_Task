//Codeforces 1512G - Short Task

#include <iostream>
#include <vector>

using namespace std;
#define c_max 10000000

void lin_sieve(vector<int>& lp) {
    int N=10000000 ;
    vector<int> pr ;
    for (int i=2;i<=N;++i) {
        if (lp[i]==0) {
            lp[i]=i ;
            pr.push_back(i) ;
        }
        for (int j=0 ; j<pr.size() && pr[j]<=lp[i] && i*pr[j]<=N ; ++j) {
            lp[i*pr[j]]=pr[j] ;
        }
    }
}

void cal_sum(vector<int>& sum, vector<int>& lp) {
    int N=10000000 ;
    for (int i=2;i<=N;++i) {
        int j=i ;
        while (j%lp[i]==0) {
            sum[i]=sum[i]*lp[i]+1 ;
            j/=lp[i] ;
        }
        sum[i]*=sum[j] ;
    }
}

void cal_ans(vector<int>& ans, vector<int>& sum) {
    for (int i=10000000;i>0;--i) {
        if (sum[i]<=10000000) ans[sum[i]]=i ;
    }
}

int main()
{
    vector<int> lp(c_max+5,0) ;
    lin_sieve(lp) ;
    vector<int> sum(c_max+5,1) ;
    cal_sum(sum,lp) ;
    vector<int> ans(c_max+5,-1) ;
    cal_ans(ans,sum) ;
    int t ;
    cin>>t ;
    for (int i=0;i<t;++i) {
        int c ;
        cin>>c ;
        cout<<ans[c]<<'\n' ;
    }
    return 0;
}
