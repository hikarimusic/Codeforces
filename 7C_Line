//Codeforces 7C - Line

#include <iostream>

using namespace std;
typedef long long ll ;

ll gcd(ll a, ll b, ll& x, ll& y) {
    if (b==0) {
        x=1 ;
        y=0 ;
        return a ;
    }
    ll x1, y1 ;
    ll m=gcd(b,a%b,x1,y1) ;
    x=y1 ;
    y=x1-(a/b)*y1 ;
    return m ;
}

int main()
{
    ll A,B,C,m,x,y ;
    cin>>A>>B>>C ;
    m=gcd(A,B,x,y) ;
    if (C%m != 0) {
        cout<<-1 ;
    }
    else {
        cout<<-x*C/m<<' '<<-y*C/m ;
    }
    return 0;
}
